{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "eqeqeq": "off",
        "no-return-assign": "off",
        "no-useless-constructor": "off",
        "no-mixed-operators": "off"
    }
}
